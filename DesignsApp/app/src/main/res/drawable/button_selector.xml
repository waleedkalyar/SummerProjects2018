<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="2dip" android:color="#898884" />
            <gradient android:angle="-90" android:startColor="#b4733e" android:endColor="#fabb3b"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="2dip" android:color="#898884" />
            <solid android:color="#f4a846"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#faac24" />
            <gradient android:angle="-180" android:startColor="#faac24" android:endColor="#faac24" />
        </shape>
    </item>
</selector>